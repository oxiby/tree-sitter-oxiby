    "description": "Oxiby grammar for tree-sitter",
    "authors": [
      {
        "name": "Jimmy Cuadra",
        "email": "jimmy@jimmycuadra.com",
        "url": "https://www.oxiby.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/oxiby/tree-sitter-oxiby"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
